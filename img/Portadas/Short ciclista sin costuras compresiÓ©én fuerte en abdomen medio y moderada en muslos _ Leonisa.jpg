    function loadsuggesteditemprices(container, items) {
        if (!items || items.length === 0) { return; }
        var data = { "f": "GetPrices" };
        data.container = container;
        if (items.length > 0) { data.items = JSON.stringify(items); }
        $.ajax({
            type: "POST",
            url: "/sp/ajax/store/ajax.aspx",
            data: data,
            datatype: "json",
            success: function (o) {
                if (o.success) {
                    rendersuggesteditemprice(container, o);
                }
            }
        });
    }

    function rendersuggesteditemprice(container, o) {
        if (o.priceobjs !== undefined) {
            var tblseries = $(container);
            $(o.priceobjs).each(function () {
                var priceobj = $(this).get(0);
                var itemid = priceobj.itemid;
                var tdprice = $(tblseries).find('.itemPrice[item="' + itemid + '"]');
                if (priceobj.PriceHTML !== "") {
                    tdprice.html(priceobj.PriceHTML);
                }
                tdprice.parent().siblings(".ajax-loader").hide();
                tdprice.siblings(".ajax-loader").hide();
            });
        }
    }
