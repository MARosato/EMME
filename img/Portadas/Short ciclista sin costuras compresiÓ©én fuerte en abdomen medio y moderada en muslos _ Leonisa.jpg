    function getOrderSummary() {

        $.ajax({
            f: "GetOrderSummary",
            type: "GET",
            data: "F=GetHeaderSummary&SiteId=2",
            url: "/sp/ajax/store/ajax.aspx",
            success: function (s) {
                var o = eval('(' + s + ')');
                
                if (!o.Success) {
                    return false;
                }
                else {
                    //   $(".acctsmry").html(o.Member);
                    
                    $(".Badge-count").html(o.cartquantity);
                    if (o.MemberLoggedIn) {
                        $("#HeaderLogin").parent().html("<a class='Masthead-action' href='" + o.MemberUrl + "'>" + $("#HeaderLogin").html() + "</a>");
                      
                    } else {
                        $("#HeaderLogin").css("visibility", "visible");                       
                       
                    }
                }
            }
        });
    }

    function popupHeaderLogin() {
        $("#header-error-label").html("");
        var popupUsername = $("#email-login");
        var popupPassword = $("#password");
        var popupPersist =$("#CT_Header_0_chkPersist");

        $.ajax({
            type: "POST",
            url: "/sp/ajax/store/ajax.aspx",
            data: {
                "F": "HeaderLogin",
                "SiteId": 2,
                "Username": popupUsername.val(),
                "Password": popupPassword.val(),
                "Persist" : popupPersist.val()
            },
            success: function (o) {
                o = typeof o === "object" ? o : (JSON.parse(o) ? JSON.parse(o) : o)
                if (typeof o === "object" && !o.Success) {
                    if (o.Error) {
                        $("#header-error-label").html(o.Error);
                    }
                    return;
                } else {
                    console.log(o)
                }

                window.top.location.href = getUnqiueUrl();
            },
            error: function (xhr, ajaxOptions, thrownError) {
                console.log(xhr.statusText);
                console.log(thrownError);
            } 
        });
    }


    function getUnqiueUrl() {
        var href = '';
        return href + (href.indexOf("?") > -1 ? "&" : "?") + 'SignedIn=Y';
    }
