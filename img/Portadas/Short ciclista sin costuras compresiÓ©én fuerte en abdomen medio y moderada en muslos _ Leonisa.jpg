<script type="75b5eead51d91d84772e3e37-text/javascript">

    var num = document.getElementById("drpqty0");
    var hnum = document.getElementById("txtQty0");
    var add = document.getElementById(num.dataset.sourceadd);
    var sub = document.getElementById(num.dataset.sourcesub);
    var min = Number(num.min);
    var max = Number(num.max);



    var event = new Event('input');

    add.addEventListener("click", function () {
        num.value = Number(num.value.trim()) + 1;
        hnum.value = num.value;
        num.dispatchEvent(event);
    });

    sub.addEventListener("click", function () {
        num.value = Number(num.value.trim()) - 1;
        hnum.value = num.value;
        num.dispatchEvent(event);
    });

    num.addEventListener("input", function () {
        calcMinMax();
    });

    function calcMinMax() {
        curr = Number(num.value.trim());
        if (num.max != "" && curr <= max) {
            sub.disabled = false;
            add.disabled = curr == max ? true : false;
        }
        if (num.min != "" && curr >= min) {
            add.disabled = false;
            sub.disabled = curr == min ? true : false;
        }

        if (curr >= max) {
            num.value = max;
            add.disabled = true;
        } else if (curr <= min) {
            num.value = min;
            sub.disabled = true;
        }

    }

    calcMinMax();

    window.onload = window.onpageshow = window.onreadystatechange = function () { num.dispatchEvent(event); }
    



</script>


