    function fb_login() {
        FB.getLoginStatus(function (response) {
            if (response.status === 'connected') {   // Logged into your webpage and Facebook.
                const fbtok = response.authResponse.accessToken
                FB.api('/me', 'GET', { "fields": "id,email" }, function (info) {
                    SocialLogin(info.email, fbtok, info.id, 'Facebook')
                });
            } else {                                 // Not logged into your webpage or we are unable to tell.
                FB.login();
                
            }
        });
    }


    function G_login() {

        gapi.load('auth2', function () {
            gauth = gapi.auth2.init({
                client_id: '535883854230-hbbj9tm1es22l234lsgkqp6ua6sn09uj.apps.googleusercontent.com',
                scope: 'profile'
            });

            // Listen for changes to current user.
            gauth.currentUser.listen(userChanged);

            // Listen for sign-in state changes.
            gauth.isSignedIn.listen(signinChanged);
            
            // Sign in the user if they are currently signed in.
            if (gauth.isSignedIn.get() !== true) {
                gauth.signIn();
            } else {

                // Start with the current live values.
                refreshValues();
            }

            


        });


        /**
     * Listener method for sign-out live value.
     *
     * @param {boolean} val the updated signed out state.
     */
        var signinChanged = function (val) {
            googleUser = val;
            updateGoogleUser();
        };

        /**
     * Listener method for when the user changes.
     *
     * @param {GoogleUser} user the updated user.
     */
        var userChanged = function (user) {
            googleUser = user;
            updateGoogleUser();
        };
        /**
     * Updates the properties in the Google User table using the current user.
     */
        var updateGoogleUser = function () {
            if (googleUser) {
                var profile = gauth.currentUser.get().getBasicProfile();
                SocialLogin(profile.getEmail(), gauth.currentUser.get().getAuthResponse().id_token, profile.getId(), 'Google')
            } else {
                console.log('error while trying to login into google account');
            }
        };

        /**
     * Retrieves the current user and signed in states from the GoogleAuth
     * object.
     */
        var refreshValues = function () {
            if (gauth) {
                googleUser = gauth.currentUser.get();
                JSON.stringify(googleUser, undefined, 2);
                gauth.isSignedIn.get();
                updateGoogleUser();
            }
        }

    }

</script>

