    function switchGender(obj) {
        var gender = $(obj).data("g");


        $.postJSON("/sp/ajax/store/ajax.aspx?F=SwitchGenderC4&GenderRequest=" + gender + "&PageId=2", function (o) {
            if (o.Success) {
                $(obj).addClass("active");
                if (isDefined(o.RedirectURL) && o.RedirectURL && o.RedirectURL.length > 0) {
                    location = o.RedirectURL;
                }
            }
        });

    }
